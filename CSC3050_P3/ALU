T_2 ;
    %wait E_0xaaaac2cb4960;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0xaaaac2ca4aa0_0, 0, 3;
    %load/vec4 v0xaaaac2cd3260_0;
