    %dup/vec4;
    %pushi/vec4 33, 0, 6;
    %cmp/u;
    %jmp/1 T_2.2, 6;
    %jmp T_2.3;
T_2.2 ;
    %load/vec4 v0xaaaac2cd37f0_0;
    %load/vec4 v0xaaaac2cd39b0_0;
    %add;
    %store/vec4 v0xaaaac2cd3630_0, 0, 32;
